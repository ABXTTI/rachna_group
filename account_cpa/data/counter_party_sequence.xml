<odoo>
    <data noupdate="1">
        <record id="seq_counter_party_payment_form_vendor" model="ir.sequence">
            <field name="name">Seq Counter Party Payment Form Vendor</field>
            <field name="code">counter.party.vendor</field>
            <field name="prefix">CPP/VEND/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
    <data noupdate="1">
        <record id="seq_counter_party_payment_form_customer" model="ir.sequence">
            <field name="name">Seq Counter Party Payment Form Customer</field>
            <field name="code">counter.party.customer</field>
            <field name="prefix">CPP/CUST/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>